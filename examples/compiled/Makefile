CC ?= gcc
CFLAGS = -std=c99 -O3 -DNDEBUG -fPIC -Wall -Wextra

.PHONY: all clean

all: libtimber_model.so libtimber_model.a

libtimber_model.so: model.c model_data.c model.h
	$(CC) $(CFLAGS) -shared -o $@ model.c -lm

libtimber_model.a: model.c model_data.c model.h
	$(CC) $(CFLAGS) -c -o model.o model.c
	ar rcs $@ model.o

clean:
	rm -f *.o *.so *.a
